    "# format and put 'data' into a pandas dataframe\n",
    "\n",
    "headers = ['Year','Month','Start (ET)','Away_Team','Away_PTS','Home_Team','Home_PTS']\n",
    "stats = pd.DataFrame(data, columns = headers)\n",
    "stats = stats.drop(columns = ['Start (ET)'])\n",
