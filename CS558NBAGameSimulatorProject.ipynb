    "from bs4 import BeautifulSoup\n",
    "from urllib.request import urlopen\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import math\n",
    "import time\n",
    "import csv"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Scrape NBA game data by using BeautifulSoup\n",
    "\n",
    "- Website: www.basketball-reference.com/\n",
    "- We get data from the past 3 seasons (2016-2019) to simulate our model\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# function for scraping the data from the url\n",
    "\n",
    "def get_data(year,month):\n",
    "    url = \"https://www.basketball-reference.com/leagues/NBA_{}_games-{}.html\".format(year,month)\n",
    "    html = urlopen(url)\n",
    "    soup = BeautifulSoup(html,'lxml')\n",
    "    rows = soup.findAll('tr')[1:]\n",
    "    game_stats = [[year,month]+[td.getText() for td in rows[i].findAll('td')][0:5]\n",
    "            for i in range(len(rows))]\n",
    "    return game_stats"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "# reading the data from the 2016-2019 NBA seasons and putting it into a list named 'data'\n",
    "\n",
    "seasons = [2017,2018,2019]\n",
    "months = ['october', 'november', 'december', 'january', 'february',\n",
    "       'march', 'april', 'may','june']\n",
    "data = []\n",
    "\n",
    "for i in range(len(seasons)):\n",
    "    for j in range(len(months)):\n",
    "        try:\n",
    "            temp_list = get_data(seasons[i], months[j])\n",
    "            #exclude empty months\n",
    "            if temp_list:\n",
    "                data = data + temp_list\n",
    "        except:\n",
    "            #if no data for that specific period\n",
    "            print(str(seasons[i]) + '-' + months[j] +' not included')"
   ]
