   "source": [
    "# get the difference & sum of two teams' scores of each game. \n",
    "# this is useful for further calculation\n",
    "\n",
    "stats['sum'] = stats['Away_PTS'].astype('int32') + stats['Home_PTS'].astype('int32')\n",
    "stats['diff'] = stats['Away_PTS'].astype('int32') - stats['Home_PTS'].astype('int32')\n",
    "\n",
    "# get the list of NBA teams\n",
    "\n",
    "teams = stats['Home_Team'].drop_duplicates().values.tolist()\n",
    "\n",
    "teams_df = pd.DataFrame(data = {'name':teams})\n",
    "\n",
    "# construct new dataframe 'games' to save our simulation-related data & results\n",
    "a_team = []\n",
    "h_team = []\n",
    "\n",
    "for item in teams:\n",
    "    for item2 in teams:\n",
    "        if item !=  item2:\n",
    "            a_team.append(item)\n",
    "            h_team.append(item2)\n",
    "\n",
    "games = pd.DataFrame(data = {'Away_Team':a_team, 'Home_Team':h_team})\n",
    "games['Game'] = games['Away_Team']+' at '+games['Home_Team']\n",
    "games = games.set_index('Game')\n",
    "games.index.name = None"
   ]
